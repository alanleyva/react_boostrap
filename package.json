{
  "name": "boostrap_starter",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/free-brands-svg-icons": "^5.11.1",
    "@fortawesome/free-regular-svg-icons": "^5.11.1",
    "@fortawesome/free-solid-svg-icons": "^5.10.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@woocommerce/woocommerce-rest-api": "^1.0.1",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.1",
    "js-cookie": "^2.2.1",
    "popper.js": "^1.15.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "react-number-format": "^4.3.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "sass": "sass --watch src/scss/:src/css",
    "update_fontawesome": "npm install --save-dev @fortawesome/fontawesome-free; cp node_modules/@fortawesome/fontawesome-free/css/all.min.css src/css/font-awesome.css; cp -r node_modules/@fortawesome/fontawesome-free/webfonts  fonts/",
    "update_bootstrap": "npm install bootstrap --save; cp node_modules/bootstrap/dist/js/bootstrap.min.js js/bootstrap.min.js; cp -r node_modules/bootstrap/scss  src/scss/bootstrap/; sass --update scss:css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
